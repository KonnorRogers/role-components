{
  "name": "role-components",
  "version": "1.1.5",
  "description": "A variety of simple components that match an aria-role",
  "type": "module",
  "source": "src/index.js",
  "main": "bundle/index.common.js",
  "module": "bundle/index.module.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "node esbuild.config.js --watch",
    "build": "rm -rf dist && node esbuild.config.js && tsc",
    "start:example": "web-dev-server",
    "deploy": "pnpm build && standard-version --release-as",
    "deploy:beta": "pnpm build && standard-version --prerelease beta",
    "deploy:patch": "pnpm build && standard-version --release-as patch",
    "deploy:minor": "pnpm build && standard-version --release-as minor",
    "deploy:major": "pnpm build && standard-version --release-as major",
    "push": "git push --follow-tags origin main && pnpm publish"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "web-components",
    "aria"
  ],
  "author": "Konnor Rogers",
  "license": "MIT",
  "devDependencies": {
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-esbuild": "^0.3.2",
    "esbuild": "^0.15.10",
    "glob": "^8.0.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.5.4",
    "lit": "^2.8.0"
  }
}
